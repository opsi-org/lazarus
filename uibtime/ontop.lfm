  object Query_top_ten_events: TSQLQuery
    FieldDefs = <>
    Database = DataModule1.IBConnection1
    Transaction = DataModule1.SQLTransaction1
    SQL.Strings = (
      'select event, count(event) '
      'from uibevent'
      'where '
      '(userid = :userid) and'
      '(pkinittime > :inittime)'
      'group by'
      'event'
      'order by'
      '2 desc'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'userid'
        ParamType = ptInput
      end    
      item
        DataType = ftUnknown
        Name = 'inittime'
        ParamType = ptInput
      end>
